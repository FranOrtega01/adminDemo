<div class="row justify-content-center">
    <h2 class="text-center">Enterprises</h2>
    <div class="col-8">
        <table class="table table-dark table-striped table-hover">
            <tr>
                <th>Name</th>
                <th>Country</th>
                <th>email</th>
            </tr>
            {{#each data}}
            <tr>
                <td>{{this.name}}</td>
                <td>{{this.country}}</td>
                <td>{{this.email}}</td>
            </tr>
            {{/each}}
        </table>
    </div>
</div>

<a href="/admin" class="m-5">
    <button class="btn btn-primary">Go back</button>
</a>